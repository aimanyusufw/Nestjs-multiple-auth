generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(uuid())
  username      String   @unique
  name          String?
  email         String   @unique
  password      String
  refresh_token String?
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
}
